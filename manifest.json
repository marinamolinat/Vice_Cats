{
    "manifest_version": 3,
    "name": "IntenTube :)",
    "version": "1.0.0",
    "description": "An extention for mindfull Youtube consumption",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "content_scripts": [
    {
      "js": [
        "Content.js"
      ],
      "matches": [
        "https://www.youtube.com/*", 
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
        
      ]
    }
  ]
  }